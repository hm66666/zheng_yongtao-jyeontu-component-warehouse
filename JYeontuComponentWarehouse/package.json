{
    "name": "@jyeontu/jvuewheel",
    "version": "0.7.3",
    "license": "MIT",
    "keyword": "vue components",
    "description": "vue组件库封装",
    "main": "lib/jvuewhell.umd.min.js",
    "scripts": {
        "dev": "set port=8081 && vue-cli-service serve --env dev",
        "serve": "set port=8081 && vue-cli-service serve",
        "debug": "set port=8081 && vue-cli-service serve --env dev --debug",
        "build": "vue-cli-service build",
        "lint": "eslint --ext .js,.vue packages",
        "lib": "vue-cli-service build --target lib --name jvuewhell --dest lib packages/index.js",
        "pub": "npm publish --access public"
    },
    "lint-staged": {
        "*.{ts,tsx,js,vue}": "eslint --fix"
    },
    "dependencies": {
        "@jyeontu/structure-jyeontu": "^1.0.1",
        "core-js": "^2.6.5",
        "element-ui": "^2.15.3",
        "highlight.js": "^11.3.1",
        "less": "3.9.0",
        "less-loader": "5.0.0",
        "v-emoji-picker": "^2.3.3",
        "vue": "^2.6.10",
        "vue-codemirror": "^4.0.6",
        "vue-highlightjs": "^1.3.3",
        "vue-resource": "^1.5.3",
        "vue-router": "^3.5.2"
    },
    "devDependencies": {
        "@vue/cli-plugin-babel": "^3.8.0",
        "@vue/cli-service": "^3.8.0",
        "compression-webpack-plugin": "4.0.0",
        "style-loader": "^3.2.1",
        "url-loader": "^4.1.1",
        "vue-style-loader": "^4.1.3",
        "vue-template-compiler": "^2.6.10"
    }
}
